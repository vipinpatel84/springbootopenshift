pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Build Started'
                withMaven(){
                    bat 'mvn clean install'
                }
            }
        }
         stage('Push to Artifactory') {
             steps {
             echo 'Setup is started'
                rtServer (
                    id: "springbootopenshift",
                    url: 'http://localhost:8082/artifactory',
                    timeout: 300
                )
                echo 'Setup is done'
                echo ' Started '
                withMaven(){
                    bat 'mvn deploy -Dmaven.test.skip=true'
                }
                echo ' end '
            }
        }                      
                
    }
}
